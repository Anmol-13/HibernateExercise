<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jtcindia.hibernate">
<class name="Customer" table="customer" >
<id name="cid" column="cid" type="int">
<generator class="increment"/>
</id>
<property name="cname" type="string"></property>
<property name="email" type="string"></property>
<property name="city" type="string"></property>
<property name="cardBal" type="double"></property>
<property name="cardType" type="string"></property>
<property name="cardNo" type="int"></property>
<property name="status" type="string"></property>
</class>
</hibernate-mapping>